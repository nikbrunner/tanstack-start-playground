{
  "name": "tanstack-start-graphql-playground",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "dev": "vinxi dev",
    "build": "vinxi build",
    "start": "vinxi start",
    "format": "prettier --write .",
    "check:format": "prettier --list-different .",
    "check:types": "npx gql.tada generate output && npx gql.tada check && tsc --project tsconfig.json",
    "gql:doctor": "npx gql.tada doctor",
    "gql:check": "npx gql.tada check",
    "gql:schema": "npx gql.tada generate schema https://swapi-graphql.netlify.app/graphql",
    "gql:turbo": "npx gql.tada turbo",
    "gql:generate": "npx gql.tada generate output"
  },
  "dependencies": {
    "@tanstack/react-query": "^5.79.0",
    "@tanstack/react-router": "^1.120.11",
    "@tanstack/react-router-devtools": "^1.120.11",
    "@tanstack/react-start": "^1.120.11",
    "gql.tada": "^1.8.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^2.6.0",
    "vinxi": "0.5.3"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@parcel/watcher": "^2.5.1",
    "@tanstack/react-query-devtools": "^5.79.0",
    "@types/node": "^22.5.4",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "autoprefixer": "^10.4.20",
    "dotenv": "^16.5.0",
    "graphql": "^16.11.0",
    "postcss": "^8.5.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "packageManager": "yarn@4.9.1"
}
